{
  "name": "opuscodium-sogo",
  "version": "4.0.0",
  "author": "Opus Codium",
  "summary": "Installs, configures, and manages the SOGo groupware",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-sogo",
  "project_page": "https://github.com/opus-codium/puppet-sogo",
  "issues_url": "https://github.com/opus-codium/puppet-sogo/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.3.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}
